//wap to read and display the elements of array
#include<stdio.h>
int main(){
    int n;//here to decxlare the size of array
    printf("enter the value of n:");
    scanf("%d",&n);
    int arr[n];
    printf("enter %d elements",n);
    for(int i=0;i<n;i++){
    scanf("%d",&arr[i]);}
    printf("elements of array are");
    for(int i=0;i<n;i++){
        printf("%d \n",arr[i]);
        
    }
    return 0;
}
